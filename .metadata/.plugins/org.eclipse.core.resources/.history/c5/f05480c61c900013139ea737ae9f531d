package week15;

public class DLList<E> implements AOPSListGeneric<E> {
	
	private class Node<E> {
		public E data;
		public Node<E> next;
		public Node<E> previous;
		
		public Node(E data, Node<E> next, Node<E> previous) {
			this.data = data;
			this.next = next;
			this.previous = previous;
		}
	}
	
	private Node<E> head;
	
	public DLList() {
		this.head = null;
	}
	
	@Override
	public void addToFront(E x) {
		if (this.head == null) {
			this.head = new Node<E>(x, null, null);
		}
		
		else {
			this.head = new Node<E>(x, null, this.head);
		}
		
	}

	@Override
	public void addToBack(E x) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void insert(E x, int position) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void replace(E x, int position) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public E get(int position) throws NoSuchListElementException {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public int size() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public boolean isEmpty() {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public boolean contains(E x) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public void remove(int position) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public int index(E x) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public AOPSListGeneric<E> sample(int s) {
		// TODO Auto-generated method stub
		return null;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
