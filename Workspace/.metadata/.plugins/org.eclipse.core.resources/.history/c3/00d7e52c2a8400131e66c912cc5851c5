
public class Week12Problem3 {
	
	public static int numPythagTriples(int N) {
		int numTriples = 0;
		for (int c = 0; c < N; c ++) {
			for (int b = c; b < N; b ++) {
				for (int a = b; a < N; a ++) {
					if (a * a + b * b == c * c) {
						numTriples ++;
					}
				}
			}
		}
		
		return numTriples;
	}

	public static void main(String[] args) {
		System.out.println(numPythagTriples(Integer.MAX_VALUE));

	}

}
